<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.opendarts.ui.x01.editor.SetX01Editor"
            default="false"
            icon="icons/view/editor.png"
            id="opendarts.editor.x01"
            name="x01 Game">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.opendarts.ui.x01.handler.SetInfoHandler"
            commandId="opendarts.cmd.setInfo">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="opendarts.cmd.setInfo"
            icon="icons/actions/info.png">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:openwis.editor.game.toolbar">
         <command
               commandId="opendarts.cmd.setInfo"
               label="Set Info"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="opendarts.cmd.setInfo"
            name="Set Info">
      </command>
   </extension>
</plugin>
